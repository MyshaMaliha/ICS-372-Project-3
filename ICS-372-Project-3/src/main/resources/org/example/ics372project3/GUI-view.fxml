<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.ics372project3.GUIController"
      stylesheets="@styles.css">

    <Label text="Car Dealership System" style="-fx-font-size: 18px; -fx-font-weight: bold;" />

    <Button text="1. Check Dealers and Vehicles" onAction="#checkDealers"/>
    <Button text="2. Enable Dealer" onAction="#enableDealer"/>
    <Button text="3. Disable Dealer" onAction="#disableDealer"/>
    <Button text="4. Add a Vehicle" onAction="#addVehicle"/>
    <Button text="5. Update Dealer Name" onAction="#updateDealerName"/>
    <Button text="6. Export JSON file" onAction="#exportJSON"/>
    <Button text="7. Transfer Inventory" onAction="#transferInventory"/>
    <Button text="8. Loan Vehicle" onAction="#loanVehicle"/>
    <Button text="9. Return Vehicle" onAction="#returnVehicle"/>
    <Button text="10. Loaned Vehicles List" onAction="#showLoanedVehicles"/>



</VBox>
